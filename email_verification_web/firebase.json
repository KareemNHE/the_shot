{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/verify-email",
        "destination": "/verify-email.html"
      },
      {
        "source": "/reset-password",
        "destination": "/reset-password.html"
      },
      {
        "source": "/__/auth/action",
        "function": "redirectAuthAction"
      },
      {
        "source": "/{__,auth}/**",
        "dynamicLinks": true
      },
      {
        "source": "/eauth5e352.page.link/**",
        "function": "redirectAuthAction"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}